@page "/"
@using BlazorApp.Shared

<PageTitle>Home</PageTitle>

<h1>Hello, world!</h1>

Welcome to your new app.

@code {
    private TrackedItem item = new TrackedItem("My Item")
    {
        Targets = 
        {
            new Target
            {
                Qty = 4,
                Frequency = TimeSpan.FromDays(1)
            },
            new Target
            {
                Qty = 1,
                Frequency = TimeSpan.FromHours(4)
            },
        }
    };
}

<BlazorApp.Client.Components.TrackedItemComponent ItemToTrack="@item" />
